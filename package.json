{
  "name": "@yuxu-palace/kun-mythos",
  "version": "0.0.0",
  "description": "A powerful TypeScript utility library by Yuxu Palace",
  "keywords": [
    "typescript",
    "utility",
    "library",
    "toolkit",
    "yuxu-palace"
  ],
  "homepage": "https://github.com/Yuxu-Palace/kun-mythos#readme",
  "bugs": {
    "url": "https://github.com/Yuxu-Palace/kun-mythos/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yuxu-Palace/kun-mythos.git"
  },
  "license": "MIT",
  "author": "Yuxu Palace",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "biome check --write && rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "dev:mf": "rslib mf-dev",
    "format": "biome format --write",
    "test:ci": "cross-env CI=true vitest run",
    "test": "vitest",
    "prepare": "husky",
    "publish:doctor": "pnpm dlx publint"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@rslib/core": "^0.10.6",
    "@types/node": "^22.16.4",
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.10.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
